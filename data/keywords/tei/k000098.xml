            <term xml:id="term-k000098-4" xml:lang="zh-latn-pinyin-nt" type="preferred" source="#biblk000098-1">yinggong</term>
            <term xml:id="term-k000098-2" xml:lang="zh-Hant" source="#biblk000098-2">影栱</term>
            <term xml:id="term-k000098-3" xml:lang="zh-Hans">影栱</term>
